{
    "compilerOptions": {
      "baseUrl": ".",
      "inlineSourceMap": true,
      "inlineSources": true,
      "module": "ESNext",
      "target": "ESNext",
      "allowJs": true,
      "noImplicitAny": true,
      "moduleResolution": "node",
      "importHelpers": true,
      "isolatedModules": true,
      "strictNullChecks": true,
      "types": ["obsidian-typings"],
      "lib": [
        "DOM",
        "ES5",
        "ES6",
        "ES7",
        "ES2018",
        "DOM.Iterable",
        "ESNext",
        "ES2022"
      ]
    },
    "include": ["**/*.ts"]
  }