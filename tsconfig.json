{
  "compilerOptions": {
    "baseUrl": ".",
    "inlineSourceMap": true,
    "inlineSources": true,
    "module": "ESNext",
    "target": "ES2023",
    "allowJs": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "isolatedModules": true,
    "strictNullChecks": true,
    "types": ["obsidian-typings"],
    "lib": [
      "DOM",
      "ES5",
      "ES6",
      "ES7",
      "ES2023",
      "DOM.Iterable",
      "DOM.AsyncIterable"
    ]
  },
  "include": ["src/**/*", "main.ts", ".deprecated/isHighlight.ts"]
}